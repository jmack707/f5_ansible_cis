apiVersion: "cis.f5.com/v1"
kind: IngressLink
metadata:
  name: vs-ingresslink-{{app_name}}
  namespace: nginx-ingress
spec:
  virtualServerAddress: {{ app.ip_address }}
  host: "{{ingress.host}}"
  iRules:
    - /Common/Shared/Proxy_Protocol_iRule
  selector:
    matchLabels:
      app: ingresslink
